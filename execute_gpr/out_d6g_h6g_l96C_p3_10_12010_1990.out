   386   1018.8 MiB      0.0 MiB                       class GPR_diag(gpf.models.GPModel):
   387                                                     r"""
   388                                                     Gaussian Process Regression.
   389                                                     This is a vanilla implementation of GP regression with a pointwise Gaussian
   390                                                     likelihood.  Multiple columns of Y are treated independently.
   391                                                     The log likelihood of this models is sometimes referred to as the 'marginal log likelihood',
   392                                                     and is given by
   393                                                     .. math::
   394                                                        \log p(\mathbf y \,|\, \mathbf f) =
   395                                                             \mathcal N\left(\mathbf y\,|\, 0, \mathbf K + \sigma_n \mathbf I\right)
   396   1018.8 MiB      0.0 MiB                           """
   397   1021.1 MiB      0.0 MiB                           def __init__(self,
   398                                                                  data: Data,
   399                                                                  kernel: Kernel,
   400   1018.8 MiB      0.0 MiB                                        mean_function: Optional[MeanFunction] = None,
   401   1018.8 MiB      0.0 MiB                                        likelihood=likelihood):
   402   1021.1 MiB      0.0 MiB                               likelihood = gpf.likelihoods.Gaussian(variance=likelihood)
   403   1021.1 MiB      0.0 MiB                               _, y_data = data
   404   1021.1 MiB      0.0 MiB                               super().__init__(kernel,
   405   1021.1 MiB      0.0 MiB                                                likelihood,
   406   1021.1 MiB      0.0 MiB                                                mean_function,
   407   1021.1 MiB      0.0 MiB                                                num_latent=y_data.shape[-1])
   408   1021.1 MiB      0.0 MiB                               self.data = data
   409                             
   410   2160.4 MiB      0.0 MiB                           def log_likelihood(self):
   411                                                         """
   412                                                         Computes the log likelihood.
   413                                                         """
   414   2160.4 MiB      0.0 MiB                               x, y = self.data
   415   2185.1 MiB      0.0 MiB                               K = self.kernel(x)
   416   2185.1 MiB      0.0 MiB                               num_data = x.shape[0]
   417   2185.1 MiB      0.0 MiB                               k_diag = tf.linalg.diag_part(K)
   418   2185.1 MiB      0.0 MiB                               s_diag = tf.convert_to_tensor(self.likelihood.variance)
   419   2185.1 MiB      0.0 MiB                               jitter = tf.cast(tf.fill([num_data], default_jitter()),
   420   2185.1 MiB      0.0 MiB                                                'float64')  # stabilize K matrix w/jitter
   421   2185.1 MiB      0.0 MiB                               ks = tf.linalg.set_diag(K, k_diag + s_diag + jitter)
   422   2185.1 MiB      0.0 MiB                               L = tf.linalg.cholesky(ks)
   423   2185.1 MiB      0.0 MiB                               m = self.mean_function(x)
   424                             
   425                                                         # [R,] log-likelihoods for each independent dimension of Y
   426   2185.1 MiB      0.2 MiB                               log_prob = multivariate_normal(y, m, L)
   427   2185.1 MiB      0.0 MiB                               return tf.reduce_sum(log_prob)
   428                             
   429   1865.8 MiB      0.0 MiB                           def predict_f(self,
   430                                                                   predict_at: tf.Tensor,
   431                                                                   full_cov: bool = False,
   432   1018.8 MiB      0.0 MiB                                         full_output_cov: bool = False):
   433                                                         r"""
   434                                                         This method computes predictions at X \in R^{N \x D} input points
   435                                                         .. math::
   436                                                             p(F* | Y)
   437                                                         where F* are points on the GP at new data points, Y are noisy observations at training data points.
   438                                                         """
   439   1865.8 MiB      0.0 MiB                               x_data, y_data = self.data
   440   1865.8 MiB      0.0 MiB                               err = y_data - self.mean_function(x_data)
   441                             
   442   2013.2 MiB      0.0 MiB                               kmm = self.kernel(x_data)
   443   2013.2 MiB      0.0 MiB                               knn = self.kernel(predict_at, full=full_cov)
   444   5469.2 MiB      0.0 MiB                               kmn = self.kernel(x_data, predict_at)
   445                             
   446   5469.2 MiB      0.0 MiB                               num_data = x_data.shape[0]
   447   5469.2 MiB      0.0 MiB                               s = tf.linalg.diag(tf.convert_to_tensor(
   448   5493.4 MiB     24.2 MiB                                   self.likelihood.variance))  #changed from normal GPR
   449                             
   450   5493.4 MiB      0.0 MiB                               conditional = gpf.conditionals.base_conditional
   451   5493.4 MiB      0.0 MiB                               f_mean_zero, f_var = conditional(
   452   5517.9 MiB     24.5 MiB                                   kmn, kmm + s, knn, err, full_cov=full_cov,
   453   5567.3 MiB     49.4 MiB                                   white=False)  # [N, P], [N, P] or [P, N, N]
   454   5567.3 MiB      0.0 MiB                               f_mean = f_mean_zero + self.mean_function(predict_at)
   455   5567.3 MiB      0.0 MiB                               return f_mean, f_var
   456                             
   457                             
   458   1018.8 MiB      0.0 MiB                       def normalize(df):
   459   1018.8 MiB      0.0 MiB                           return np.array((df - df.mean()) / df.std()).reshape(len(df), 1)
   460                             
   461                             
   462   2111.3 MiB      0.0 MiB                       def denormalize(y_pred, df):
   463   2111.3 MiB      0.0 MiB                           return np.array((y_pred * df.std()) + df.mean())
   464                             
   465                             
   466   1021.1 MiB      0.0 MiB                       def bounded_parameter(low, high, param):
   467                                                     """Make parameter tfp Parameter with optimization bounds."""
   468   1021.1 MiB      0.0 MiB                           affine = tfb.AffineScalar(shift=tf.cast(low, tf.float64),
   469   1021.1 MiB      0.0 MiB                                                     scale=tf.cast(high - low, tf.float64))
   470   1021.1 MiB      0.0 MiB                           sigmoid = tfb.Sigmoid()
   471   1021.1 MiB      0.0 MiB                           logistic = tfb.Chain([affine, sigmoid])
   472   1021.1 MiB      0.0 MiB                           parameter = gpf.Parameter(param, transform=logistic, dtype=tf.float64)
   473   1021.1 MiB      0.0 MiB                           return parameter
