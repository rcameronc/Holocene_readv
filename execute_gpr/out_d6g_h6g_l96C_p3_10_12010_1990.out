   528   1021.1 MiB      0.0 MiB                                   active_dims=active_dims,
   529   1021.1 MiB      0.0 MiB                                   variance=variance,
   530   1021.1 MiB      2.1 MiB                                   lengthscale=lengthscale,
   531                                                         )
   532                             
   533   5469.2 MiB      0.0 MiB                           def haversine_dist(self, X, X2):
   534   5469.2 MiB      0.0 MiB                               pi = np.pi / 180
   535   5469.2 MiB      0.0 MiB                               f = tf.expand_dims(X * pi, -2)  # ... x N x 1 x D
   536   5469.2 MiB      0.0 MiB                               f2 = tf.expand_dims(X2 * pi, -3)  # ... x 1 x M x D
   537  12381.2 MiB   6912.0 MiB                               d = tf.sin((f - f2) / 2)**2
   538  12381.2 MiB      0.1 MiB                               lat1, lat2 = tf.expand_dims(X[:, 0] * pi, -1), \
   539  12381.2 MiB      0.0 MiB                                           tf.expand_dims(X2[:, 0] * pi, -2)
   540  15837.2 MiB   3456.0 MiB                               cos_prod = tf.cos(lat2) * tf.cos(lat1)
   541  19293.2 MiB   3456.0 MiB                               a = d[:, :, 0] + cos_prod * d[:, :, 1]
   542  22749.2 MiB   3456.0 MiB                               c = tf.asin(tf.sqrt(a)) * 6371 * 2
   543  22749.2 MiB      0.0 MiB                               return c
   544                             
   545   5469.2 MiB    122.8 MiB                           def scaled_squared_euclid_dist(self, X, X2):
   546                                                         """
